# Test Environment Configuration

# Server Configuration
PORT=3001
HOST=127.0.0.1
NODE_ENV=test

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGINS=*

# Rate Limiting (more lenient for tests)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# Server Timeout
SERVER_TIMEOUT=10000

# Vector Database Configuration (Test)
VECTOR_DB_PROVIDER=faiss
VECTOR_DB_CONNECTION_STRING=
VECTOR_DB_API_KEY=
VECTOR_DB_INDEX_NAME=test-rag-index
VECTOR_DB_DIMENSION=384

# Metadata Database Configuration (Test)
METADATA_DB_PROVIDER=sqlite
METADATA_DB_CONNECTION_STRING=sqlite://./data/test/metadata.db

# Redis Cache Configuration (Test DB)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=15
REDIS_MAX_MEMORY=64mb
REDIS_EVICTION_POLICY=allkeys-lru

# Cache TTL Configuration (shorter for tests)
CACHE_TTL_QUERY_RESULTS=300
CACHE_TTL_EMBEDDINGS=600
CACHE_TTL_HEALTH_CHECKS=60

# Embedding Configuration (Test)
EMBEDDING_PROVIDER=sentence-transformers
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_API_KEY=
EMBEDDING_DIMENSION=384
EMBEDDING_BATCH_SIZE=8
EMBEDDING_TIMEOUT=10000

# Search Configuration
SEARCH_DEFAULT_TOP_K=5
SEARCH_MAX_TOP_K=20
SEARCH_SIMILARITY_THRESHOLD=0.5

# Hybrid Search Configuration
HYBRID_SEARCH_ENABLED=true
HYBRID_SEARCH_VECTOR_WEIGHT=0.7
HYBRID_SEARCH_KEYWORD_WEIGHT=0.3

# Reranking Configuration
RERANKING_ENABLED=false
RERANKING_MODEL=

# Monitoring Configuration (Disabled for tests)
METRICS_ENABLED=false
METRICS_PORT=9091
METRICS_PATH=/metrics

# Logging Configuration (Test)
LOG_LEVEL=error
LOG_FORMAT=json
LOG_FILE=

# Health Check Configuration
HEALTH_CHECK_INTERVAL=10000
HEALTH_CHECK_TIMEOUT=2000