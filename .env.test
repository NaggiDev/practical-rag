NODE_ENV=test
LOG_LEVEL=error

# Database Configuration
DATABASE_METADATA_CONNECTION_STRING=sqlite://./data/test/metadata_test.db
DATABASE_VECTOR_PROVIDER=faiss
DATABASE_VECTOR_INDEX_NAME=test-rag-index

# Cache Configuration
CACHE_PROVIDER=memory
CACHE_TTL=300
CACHE_MAX_SIZE=1000
CACHE_REDIS_HOST=localhost
CACHE_REDIS_PORT=6379
CACHE_REDIS_DB=15

# API Configuration
API_PORT=0
API_CORS_ENABLED=true
API_RATE_LIMIT_ENABLED=false

# Embedding Configuration
EMBEDDING_PROVIDER=local
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIMENSIONS=384
EMBEDDING_BATCH_SIZE=32

# Test Configuration
TEST_TIMEOUT=30000
TEST_DATA_PATH=./src/test/test-data