{
    "name": "fast-rag-system",
    "version": "1.0.0",
    "description": "Fast and efficient Retrieval-Augmented Generation system",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:integration": "ts-node src/test/integration/runIntegrationTests.ts",
        "test:integration:e2e": "jest --testPathPattern=endToEnd.integration.test.ts --verbose",
        "test:integration:performance": "jest --testPathPattern=performance.integration.test.ts --verbose",
        "test:integration:load": "jest --testPathPattern=loadTesting.integration.test.ts --verbose",
        "test:integration:failures": "jest --testPathPattern=failureScenarios.integration.test.ts --verbose",
        "test:performance": "jest --testPathPattern=performanceValidation.test.ts --verbose --testTimeout=600000",
        "test:performance:run": "ts-node src/scripts/runPerformanceTests.ts",
        "test:all": "npm run test && npm run test:integration",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "migrate": "ts-node scripts/migrate.ts",
        "migrate:up": "ts-node scripts/migrate.ts up",
        "migrate:down": "ts-node scripts/migrate.ts down",
        "migrate:reset": "ts-node scripts/migrate.ts reset",
        "db:setup": "ts-node src/data/migrations/setup.ts setup",
        "db:teardown": "ts-node src/data/migrations/setup.ts teardown"
    },
    "keywords": [
        "rag",
        "retrieval",
        "generation",
        "ai",
        "search",
        "vector"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@huggingface/inference": "^2.6.4",
        "@pinecone-database/pinecone": "^1.1.2",
        "@qdrant/js-client-rest": "^1.7.0",
        "@xenova/transformers": "^2.6.0",
        "axios": "^1.6.2",
        "cheerio": "^1.0.0-rc.12",
        "commander": "^11.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "faiss-node": "^0.5.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "joi": "^17.11.0",
        "mammoth": "^1.6.0",
        "marked": "^11.1.1",
        "mongodb": "^6.3.0",
        "openai": "^4.20.1",
        "pdf-parse": "^1.1.1",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "sqlite3": "^5.1.6",
        "uuid": "^9.0.1",
        "weaviate-ts-client": "^1.4.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/commander": "^2.12.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.10.5",
        "@types/pdf-parse": "^1.1.4",
        "@types/pg": "^8.10.9",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint": "^8.55.0",
        "jest": "^29.7.0",
        "supertest": "^7.1.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}